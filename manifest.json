{
    "manifest_version": 3,
    "name": "GIGA GAMER TRANSLATOR v0.5.01 BETA",
    "version": "0.5",
    "description": "Add funny translations to your reading experience!",

    "permissions": ["storage", "activeTab", "scripting"],

    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["./scripts/content.js"],
          "run_at": "document_end"
        }
      ],
      "icons": {
        "16": "/images/image16.png",
        "32": "/images/image32.png",
        "64": "/images/image64.png",
        "128": "/images/image128.png"
      }, 

    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/image16.png",
          "32": "/images/image32.png",
          "64": "/images/image64.png",
          "128": "/images/image128.png"
    }
  }
}